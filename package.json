{
  "name": "serverless-disable-request-validators",
  "version": "1.0.1",
  "description": "Serverless plugin to disable Request Validators from API Gateway",
  "author": "Jardel Weyrich",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    },
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "perf",
        "section": "Performance Improvements"
      },
      {
        "type": "refactor",
        "section": "Maintenance Improvements"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jweyrich/serverless-disable-request-validators.git"
  },
  "bugs": {
    "url": "https://github.com/jweyrich/serverless-disable-request-validators/issues"
  },
  "homepage": "https://github.com/jweyrich/serverless-disable-request-validators#readme",
  "keywords": [
    "serverless",
    "api gateway",
    "request validator",
    "serverless plugin"
  ],
  "scripts": {
    "prepare-release": "standard-version",
    "prepublish": "npm run build",
    "precommit": "npm run test",
    "build": "rm -rf dist && tsc",
    "pretest": "npm run lint",
    "lint": "eslint ./src --ext .ts",
    "test": "jest"
  },
  "engines": {
    "node": ">=12.16.0"
  },
  "peerDependencies": {
    "serverless": ">=2.0.0",
    "typescript": ">=4.0.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "jest": "24.5.0",
    "ts-jest": "24.0.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
